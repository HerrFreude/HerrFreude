! ...
! Xresources
! Update with:
! xrdb ~/.Xresources
! xrdb -merge  ~/.Xresources
!...

! Xterm ========================
XTerm.termName: xterm-256color

! Set font and fontsize
XTerm*faceName: Monospace
XTerm*faceSize: 12
xterm*font: *-fixed-*-*-*-18-*

! Set  utf-8 support
XTerm.vt100.locale: true

! Set color theme
*background: #1c272c
 *foreground: #f2f8f9
 *color0:     #000000
 *color1:     #d04767
 *color2:     #aece91
 *color3:     #efc002
 *color4:     #7a7ab0
 *color5:     #963c59
 *color6:     #418179
 *color7:     #bebebe
 *color8:     #666666
 *color9:     #ef8171
 *color10:    #e5f779
 *color11:    #fff796
 *color12:    #4186be
 *color13:    #f56789
 *color14:    #71bebe
 *color15:    #ffffff
! Xclock Styling
XClock*foreground: #fbfbfb
XClock*background: #1c272c
XClock*update: 1
XClock*geometry:          320x320+1020+12
XClock.Clock.majorColor:  rgba:f0/f0/19/7b
XClock.Clock.minorColor:  rgba:a0/c0/f0/c0
XClock.Clock.hourColor:   rgba:c9/66/11/72
XClock.Clock.minuteColor: rgba:00/82/9f/72
XClock.Clock.secondColor: rgba:50/93/30/6f

